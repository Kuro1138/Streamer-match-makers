            <div class="row carousel-row-item">
              <div class="col-lg-6 col-md-6 contributor-item-left">
                <div id="carouselIndicators" class="carousel slide" data-ride="carousel">
                  <div class="carousel-inner">
                    <div class="carousel-item active">
                      <img class="d-block w-100" src="https://images.theconversation.com/files/342194/original/file-20200616-23213-17thq4v.jpg?ixlib=rb-1.1.0&rect=7%2C37%2C5001%2C3109&q=45&auto=format&w=754&fit=clip" alt="First slide" width="auto" height="250px">
                    </div>
                    <div class="carousel-item">
                      <img class="d-block w-100" src="https://www.nodesagency.com/wp-content/uploads/2018/12/London-employee-developer.jpg" alt="Second slide" width="auto" height="250px">
                    </div>
                    <div class="carousel-item">
                      <img class="d-block w-100" src="https://d33wubrfki0l68.cloudfront.net/d79ba4b37450a5f20ee971d19354df81fe1b227a/a330b/en/blog/uploads/experienced-web-developer-doing-backend-development.jpg" alt="Third slide" width="auto" height="250px">
                    </div>
                  </div>
                  <a class="carousel-control-prev" href="#carouselIndicators" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                  </a>
                  <a class="carousel-control-next" href="#carouselIndicators" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                  </a>
                  <button class="btn join-now-button" onclick="window.open('<%= process.env.DISCORD_INVITE_URL %>')">JOIN NOW</button>
                </div>
              </div>
              <div class="col-lg-5 col-md-6 ml-md-0 contributor-item-right">
                <div class="contributor-name" id="carousel-slide-title"></div>
                <div class="contributor-title" id="carousel-slide-subtitle"></div>
                <div class="contributor-desc" id="carousel-slide-desc"></div> 
              </div>
            </div>
            <ol class="carousel-indicators">
              <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselIndicators" data-slide-to="1"></li>
              <li data-target="#carouselIndicators" data-slide-to="2"></li>
            </ol>
